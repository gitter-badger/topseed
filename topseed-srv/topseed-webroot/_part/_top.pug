#appbar
	#appbar--brand(onclick='openDrawer()') &nbsp; ☰ &nbsp;
		span.mui--hidden-xs Topseed&trade;
			span &trade;
	ul#appbar--menu.mui-list--inline
		li
			a(href='/page/one/') One
		li
			a(href='/page/two/') Two

#sidedrawer
	#sidedrawer--brand &nbsp; ☰ &nbsp; Topseed&trade;
	.mui-divider
	ul#sidedrawer--menu.mui-list--unstyled
		li
			a(href='/page/one/') One
		li
			a(href='/page/two/') Two

script.
	function openDrawer(px, e) {
		$('#sidedrawer').css('transform', 'translateX(201px)')
	}
	function closeDrawer(e) {
		$('#sidedrawer').css('transform', 'translateX(0px)')
	}			
